{
	"name": "swizzle-params",
	"version": "1.1.1",
	"description": "swizzle config parameter values by stack name",
	"main": "./lib/index.js",
	"author": "javadoug",
	"license": "MIT",
	"bin": {
		"swizzle": "./lib/bin/swizzle-cli.js"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-register": "^6.26.0",
		"coveralls": "^3.0.0",
		"mocha": "^5.0.1",
		"mocha-lcov-reporter": "^1.3.0",
		"nsp": "^3.2.1",
		"nyc": "^11.4.1"
	},
	"scripts": {
		"test": "nyc --reporter text --reporter lcovonly mocha test --require babel-register",
		"test-report": "npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"build": "babel src -d lib --source-maps",
		"clean": "rm -rf lib example-project/node_modules swizzle-params-1.0.22.tgz swizzle.json",
		"devexample": "npm run clean && npm run build && npm pack . && cd example-project && npm i ../swizzle-params-1.0.22.tgz"
	},
	"babel": {
		"presets": [
			"env"
		],
		"plugins": [
			"transform-class-properties"
		]
	},
	"dependencies": {
		"commander": "^2.14.1",
		"fs-extra": "^5.0.0",
		"inquirer": "^5.1.0",
		"lodash.defaultsdeep": "^4.6.0",
		"lodash.omit": "^4.5.0",
		"lodash.pick": "^4.4.0",
		"lodash.snakecase": "^4.1.1",
		"lodash.uniq": "^4.5.0",
		"os-homedir": "^1.0.2"
	},
	"repository": "https://github.com/javadoug/swizzle-params.git"
}
